{
  "name": "@privy-io/react-auth",
  "version": "1.30.3",
  "description": "React client for the Privy Auth API",
  "keywords": [
    "authentication",
    "authorization",
    "identity",
    "privacy",
    "privy",
    "user data",
    "web3"
  ],
  "homepage": "https://github.com/privy-io/react-auth",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/privy-io/react-auth.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npx tsup --clean --minify",
    "watch": "npx tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest --testMatch \"**/test/unit/**/*.test.ts\"",
    "test-integration": "jest --testMatch \"**/test/integration/**/*.test.ts\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" && prettier --check \"src/**/*.{ts,tsx,js,jsx}\" && npx tsc --noEmit",
    "format": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix && prettier --write \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.7.1",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@ethersproject/strings": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "@heroicons/react": "^2.0.18",
    "@metamask/eth-sig-util": "^6.0.0",
    "@walletconnect/ethereum-provider": "^2.8.6",
    "@walletconnect/modal": "^2.5.9",
    "@walletconnect/qrcode-modal": "^1.8.0",
    "base64-js": "^1.5.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "eventemitter3": "^5.0.1",
    "jose": "^4.11.2",
    "js-cookie": "^3.0.5",
    "libphonenumber-js": "^1.10.14",
    "lokijs": "^1.5.12",
    "md5": "^2.3.0",
    "ofetch": "^1.0.1",
    "pino-pretty": "^10.0.0",
    "qrcode": "^1.5.1",
    "react-device-detect": "^2.2.2",
    "styled-components": "^5.3.6",
    "tinycolor2": "^1.6.0",
    "uuid": ">=8 <10",
    "web3-core": "^1.8.0",
    "web3-core-helpers": "^1.8.0",
    "wicg-inert": "^3.1.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@saithodev/semantic-release-backmerge": "^2.1.2",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^29.1.2",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.2",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "tsup": "^6.2.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "bugs": {
    "url": "https://github.com/privy-io/react-auth/issues"
  },
  "directories": {
    "test": "test"
  },
  "author": "privy.io",
  "browserslist": [
    "defaults",
    "node >= 18",
    "not op_mini all"
  ]
}
